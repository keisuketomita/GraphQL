<h1>ブログ一覧</h1>
<p><%= notice %></p>
<table>
  <tr>
    <th>タイトル</th>
    <th>内容</th>
  </tr>
<% @messages.each do |message| %>
  <tr>
    <td><%= message.content %></td>
    <td><%= link_to 'ブログを編集する', edit_message_path(message.id) %></td>
  </tr>
<% end %>
</table>
<br><%= link_to '新しくブログを作成する', new_message_path %>
